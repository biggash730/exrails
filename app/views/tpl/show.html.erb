<div class="panel panel-primary ex-form">
  <div class="panel-heading">
    <button type="button" class="close" ng-click="cancel()">
      <span class="glyphicon glyphicon-remove"></span>
    </button>
    <h3 class="panel-title">Show {{ model.display }}</h3>
  </div>
  <div class="panel-body">
    <ex-form ng-model="record" columns="formColumns" lookups="lookups" disabled="true">
    </ex-form>
  </div>
  <div class="panel-footer">
    <button class="btn btn-default btn-sm" ng-click="cancel()">
      <span class="glyphicon glyphicon-remove"></span>
      Close
    </button>
    <button class="btn btn-info btn-sm" ng-click="edit()">
      <span class="glyphicon glyphicon-pencil"></span>
      Edit
    </button>
  </div>
</div>